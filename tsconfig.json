{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
